import java.util.Scanner;

public class fibonnaci{
	public static void main(String[] args){
		Scanner show = new Scanner (System.in);
		System.out.print("Masukkan jumlah deret Fibonnaci : ");
		int s = show.nextInt();
		long angka[]= new long[s];
		
		angka[0]=0;
		angka[1]=1;
		angka[2]=2;
		
		for(int i=2; i<s; i++){
			angka[1]=angka[i-1]+angka[i-2];
		}
		for(int i=0; i<s; i++){
			System.out.print(angka[i]+ " ");
		}
	}
}
